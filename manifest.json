{
  "name": "Wiki Popup",
  "version": "1.0",
  "description": "Wikipedia popup for selected text",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage"],
  "commands": {
    "selected-text": {
        "suggested_key": {
        "default": "Alt+W"
        },
        "description": "Invoke Wikipedia Popup"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
	"default_popup": "settings.html"
  }
}
